  "native": {
    "loqedConfig": "",
    "port": 9005,
    "callbackUrl": "http://192.168.178.22"
  },
  "objects": [],
  "instanceObjects": [
    {
      "_id": "info",
      "type": "channel",
      "common": {
        "name": "Information"
      },
      "native": {}
    },
    {
      "_id": "info.connection",
      "type": "state",
      "common": {
        "name": "Connected to lock",
        "role": "indicator.connected",
        "type": "boolean",
        "write": false,
        "read": true
      },
      "native": {}
    },
    {
      "_id": "lockMotor",
      "type": "channel",
      "common": {
        "name": "Control the lock"
      },
      "native": {}
    },
    {
      "_id": "lockMotor.currentPosition",
      "type": "state",
      "common": {
        "name": "Current position",
        "role": "text",
        "type": "string",
        "write": false,
        "read": true,
        "states": [
          "DAY_LOCK",
          "OPEN",
          "NIGHT_LOCK",
          "UNKNOWN"
        ]
      },
      "native": {}
    },
    {
      "_id": "lockMotor.goToPosition",
      "type": "state",
      "common": {
        "name": "Go to position",
        "role": "text",
        "type": "string",
        "write": true,
        "read": true,
        "states": [
          "DAY_LOCK",
          "OPEN",
          "NIGHT_LOCK"
        ]
      },
      "native": {}
    },
    {
      "_id": "lockMotor.homekitLockCurrentState",
      "type": "state",
      "common": {
        "name": "Current position as HomeKit value",
        "role": "state",
        "type": "number",
        "write": false,
        "read": true,
        "states": { 
          "0": "unlock/unsecured",
          "1": "lock/secured",
          "2": "jammed",
          "3": "unknown"
        }
      },
      "native": {}
    },
    {
      "_id": "lockMotor.homekitLockTargetState",
      "type": "state",
      "common": {
        "name": "Go to position as HomeKit value",
        "role": "state",
        "type": "number",
        "write": true,
        "read": true,
        "states": {
          "0": "unlock/unsecured",
          "1": "lock/secured"
        }
      },
      "native": {}
    },
    {
      "_id": "lockMotor.simpleLockUnlock",
      "type": "state",
      "common": {
        "name": "False = lock, True = unlock",
        "role": "state",
        "type": "boolean",
        "write": true,
        "read": true
      },
      "native": {}
    },
    {
      "_id": "lockStatus",
      "type": "channel",
      "common": {
        "name": "Status of the lock"
      },
      "native": {}
    },
    {
      "_id": "lockStatus.batteryPercentage",
      "type": "state",
      "common": {
        "name": "Battery level",
        "role": "level",
        "type": "number",
        "write": false,
        "read": true,
        "unit": "%"
      },
      "native": {}
    }
  ]
}
